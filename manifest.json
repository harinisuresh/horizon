{
  "manifest_version": 2,

  "name": "Horizon",
  "description": "Automatic Alt-text for Visually Impaired Users",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
